
 org(table){
        let arr=table
        let find=false;
        let operand=0;
        for (let index = 0; index < array.length; index++) {
            const element = array[index];
            if (!find ) 
                if (element.instName='ORG') {
                    operand=element.operand[0];
                    find=true;
                }
            
            else 
                if (element.instName='ORG'){
                    arr.splice(index,index);
                }
        }
        return {table:arr,op:operand};
    }

